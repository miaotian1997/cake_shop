body,div,p,ul,li,table,dl,dt,tr,td,h1,h2,h3,h4,h5,{
	padding:0;
	margin:0;
}
@media screen and (max-width:1230px){
  #banner{
		width:1230px !important;
	}
	#banner>a img{
		width:1230px !important;
	}
}
